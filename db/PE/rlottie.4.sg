// https://github.com/Samsung/rlottie
meta("library", "rlottie");

function detect() {
    if (PE.isLibraryPresentExp(/^rlottie/i)) {
        bDetected = true;
    } else if (PE.isExportFunctionPresentExp(/^(\?|_)+.{6,}rlottie/)) {
        sOptions = "EAT, static";
        bDetected = true;
    }

    return result();
}