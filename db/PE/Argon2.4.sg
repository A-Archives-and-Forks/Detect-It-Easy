// https://github.com/P-H-C/phc-winner-argon2
meta("library", "Argon2");

function detect() {
    if (PE.isFunctionPresent("argon2_hash")) {
        bDetected = true;
    } else if (PE.isExportFunctionPresent("argon2_hash")) {
        sOptions = "static"; // EAT
        bDetected = true;
    }

    return result();
}