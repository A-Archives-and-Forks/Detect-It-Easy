// https://github.com/dpirch/libfvad
meta("library", "libfvad");

function detect() {
    if (PE.isLibraryPresentExp(/fvad\./i)) {
        bDetected = true;
    } else if (PE.isExportFunctionPresentExp(/^fvad_/)) {
        sOptions = "EAT, static";
        bDetected = true;
    }

    return result();
}