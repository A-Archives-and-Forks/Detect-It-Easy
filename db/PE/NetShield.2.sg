meta("protector", "NetShield");

function detect() {
    if (PE.isNet()) {
        if (PE.isSectionNamePresent("!Sugar") && PE.isNetGlobalCctorPresent()) {
            bDetected = true;

            if (PE.isNetObjectPresent("ConfusedByAttribute")) {
                sOptions = sOptions.append("fake signs");
            }
        }

    }

    return result();
}