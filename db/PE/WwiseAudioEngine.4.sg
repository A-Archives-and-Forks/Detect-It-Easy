// https://www.audiokinetic.com/en/wwise/overview/
meta("library", "Wwise Audio Engine");

function detect() {
    if (PE.isFunctionPresent("g_pAKPluginList")) {
        bDetected = true;
    } else if (PE.isExportFunctionPresent("g_pAKPluginList")) {
        sOptions = "EAT, static";
        bDetected = true;
    }

    return result();
}