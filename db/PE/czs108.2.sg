meta("packer", "czs108's Windows PE Packer");

function detect() {
    bDetected = (PE.getNumberOfImports() === 1 && PE.getImportLibraryName(0) === "Kernel32.dll" && PE.isSectionNamePresent(".shell"));

    return result();
}