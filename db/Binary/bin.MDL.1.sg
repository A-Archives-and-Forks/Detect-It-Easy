// https://developer.valvesoftware.com/wiki/MDL
meta("format", "MDL");

function detect() {
    if (Binary.compare("'IDST'")) {
        var idstVersion = Binary.getString(4, 1).trim();

        if (/^[0-9]{1}/.test(idstVersion)) {
            if (X.isVerbose()) sVersion = idstVersion;
            bDetected = true;
        }
    }

    return result();
}
