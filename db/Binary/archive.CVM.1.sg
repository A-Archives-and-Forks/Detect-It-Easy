// Author: BJNFNE <bjnfne@web.de>
// Used by PlayStation 2 games

// https://amicitia.miraheze.org/wiki/CVM
meta("archive", "CVM");

function detect() {
    if (Binary.compare("'CVMH'")) {
        var versionNumber = Binary.getString(68, 4).trim();

        if (/^[0-9\.]{4}/.test(versionNumber)) {
            if (X.isVerbose()) sVersion = versionNumber;
            bDetected = true;
        }
    }

    return result();
}