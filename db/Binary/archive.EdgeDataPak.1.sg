// Header of EdgeDataPak (2004) is based on CKP's (2002/2003) format

meta("archive", "EdgeDataPak (.EDP)");

function detect() {
    if (Binary.compare("'.EDP'0001")) {
        bDetected = true;
    }

    return result();
}