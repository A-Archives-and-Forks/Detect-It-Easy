// https://www.psdevwiki.com/ps3/Playstation_Update_Package_(PUP)
meta("archive", "PlayStation Update Package");

function detect() {
    if (Binary.compare("'SCEUF'0000....000000")) {
        sVersion = "PS3/PS Vita";
        bDetected = true;
    } else if (Binary.compare("'SLB2'..0000000000....02")) {
        sVersion = "PS4/PS5";
        bDetected = true;
    }

    return result();
}