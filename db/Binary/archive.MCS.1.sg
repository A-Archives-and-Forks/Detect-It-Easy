meta("archive", "MCS");

function detect() {
    if (Binary.compare("'AHFFMACS0200'")) {
        var game = Binary.getString(15, 12).trim();

        if (/^[A-Z/!]{12}/.test(game)) {
            if (X.isVerbose()) sOptions = game;
            bDetected = true;
        }
    }

    return result();
}