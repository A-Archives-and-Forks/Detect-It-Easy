// https://learn.microsoft.com/en-us/cpp/build/reference/ilk-name-incremental-database-file
meta("format", "Name incremental database file (.ILK)");

function detect() {
    if (Binary.compare("'Microsoft Linker Database'0a0a071a000000")) {
        bDetected = true;
    }

    return result();
}