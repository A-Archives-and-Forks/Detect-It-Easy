// Used by EvilEngine games

// https://heavyironmodding.org/wiki/EvilEngine/HIP_(File_Format)
meta("archive", "HIP archive");

function detect() {
    if (Binary.compare("'HIPA'00000000")) {
        sVersion = "HIP/HOP";
        bDetected = true;
    }

    return result();
}