// https://www.wiki.sc4devotion.com/index.php?title=FSH_Format
// https://rewiki.miraheze.org/wiki/EA_SSH_FSH_Image_(Type_1)
meta("image", "FSH");

function detect() {
    if (Binary.compare("'SHPI'......00..000000")) {
        sOptions = "PC";
        bDetected = true;
    }

    return result();
}
