meta("format", "LCA");

function detect() {
    if (Binary.compare("4C 45 47 4F")) {
        var vcaFile = Binary.getString(85, 12).trim();

        if (/^[A-Z0-9/.]{12}$/.test(vcaFile)) {
            if (X.isVerbose()) sOptions = vcaFile;
            bDetected = true;
        }
    }

    return result();
}