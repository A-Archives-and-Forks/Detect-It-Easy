// https://learn.microsoft.com/en-us/windows/win32/sysinfo/registry-hives
meta("format", "Windows Registry Hive (.HIV)");

function detect() {
    if (Binary.compare("'regf'..0000000..00000")) {
        bDetected = true;
    }

    return result();
}