// https://www.flatpyramid.com/3d-model/file-formats/shockwave-3d-w3d/
meta("format", "Shockwave 3D");

function detect() {
    if (Binary.compare("'IFX'")) {
        sVersion = "W3D";
        bDetected = true;
    }

    return result();
}