meta("format", "VLK");

function detect() {
    if (Binary.compare("'@VLK@2.5.0'")) {
        var versionNumber = Binary.getString(5, 5).trim();

        if (/^[0-9\.]{5}$/.test(versionNumber)) {
            sVersion = versionNumber;
            bDetected = true;
        }
    }

    return result();
}