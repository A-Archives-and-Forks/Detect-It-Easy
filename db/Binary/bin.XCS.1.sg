// https://wiki.multimedia.cx/index.php/Engines/XPAT#XCS
meta("format", "XPAT Compiled Script (.XCS)");

function detect() {
    if (Binary.compare("58 43 53")) {
        var platform = Binary.getString(4, 2).trim();

        if (/^[A-Z]{2}$/.test(platform)) {
            sVersion = platform;
            bDetected = true;
        }
    }

    return result();
}