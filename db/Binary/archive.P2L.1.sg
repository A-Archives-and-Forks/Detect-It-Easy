// https://wiki.multimedia.cx/index.php/Engines/PSM
meta("archive", "PSM Soundsystem (.P2L)");

function detect() {
    if (Binary.compare("'PSMLST PSM'")) {
        var versionNumber = Binary.getString(11, 4).trim();

        if (/^[0-9\.]{4}$/.test(versionNumber)) {
            sVersion = versionNumber;
            bDetected = true;
        }
    }

    return result();
}