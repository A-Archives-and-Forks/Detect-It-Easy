// https://www.psdevwiki.com/ps3/PBP
meta("archive", "PlayStation Update Package");

function detect() {
    if (Binary.compare("00 50 42 50")) {
        var psf = Binary.getString(41, 3).trim();

        if (/^[A-Z]{3}/.test(psf)) {
            if (X.isVerbose()) sOptions = psf;
            bDetected = true;
        }
    }

    return result();
}