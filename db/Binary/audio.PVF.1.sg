// https://fileinfo.com/extension/pvf
meta("audio", "Portable Voice Format (.PVF)");

function detect() {
    if (Binary.compare("'PVF'")) {
        var freq = Binary.getString(7, 5).trim();

        if (/^[0-9]{5}/.test(freq)) {
            if (X.isVerbose()) sVersion = freq;
            bDetected = true;
        }

    }

    return result();
}