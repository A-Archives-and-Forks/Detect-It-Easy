meta("format", "iTunes Library");

function detect() {
    if (Binary.compare("'hdfm'00000090")) {
        var versionNumber = Binary.getString(17, 8).trim();

        if (/^[0-9/.]{8}$/.test(versionNumber)) {
            if (X.isVerbose()) sVersion = versionNumber;
            bDetected = true;
        }
    }

    return result();
}