// https://www.3dbrew.org/wiki/SHBIN
meta("format", "Shader Binary");

function detect() {
    if (Binary.compare("'DVLB'")) {
        var dvlp = Binary.getString(12, 4).trim();

        if (/^[A-Z]{4}/.test(dvlp)) {
            if (X.isVerbose()) sOptions = dvlp;
            bDetected = true;
        }
    }

    return result();
}