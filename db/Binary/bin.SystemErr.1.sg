// Used in Coktel Vision games

meta("format", "System_Err");

function detect() {
    if (Binary.compare("'System_Err'")) {
        var errorLine = Binary.getString(11, 32).trim();

        if (/^[ A-Za-z0-9\-\:\\\.]{1,}$/.test(errorLine)) {
            if (X.isVerbose()) sOptions = errorLine;
            bDetected = true;
        }
    }

    return result();
}